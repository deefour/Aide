{
    "name": "deefour/aide",
    "description": "A set of classes to assist Laravel and Silex development.",
    "version": "0.1.0",
    "license": "MIT",
    "keywords": ["deefour", "eloquent", "silex", "validation", "persistence", "laravel"],
    "authors": [
        {
            "name": "Jason Daly",
            "email": "jason@deefour.me",
            "homepage": "http://deefour.me"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "support": {
        "issues": "https://github.com/deefour/Aide/issues",
        "email": "jason@deefour.me"
    },
    "require": {
        "illuminate/database": "4.1.*",
        "illuminate/validation": "4.1.*",
        "silex/silex": "~1.2",
        "twig/twig": ">=1.8,<2.0-dev",
        "symfony/twig-bridge": "~2.3"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Deefour\\Aide\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
          "stubs/"
        ],
        "psr-4": {
            "Deefour\\Aide\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}